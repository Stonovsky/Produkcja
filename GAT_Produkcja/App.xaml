    <Application.Resources>
        <ResourceDictionary>
            
            <!--#region DataGrid Filtry -->

            <!--#region Wartosci -->
            <ControlTemplate x:Key="FilterValuesWithPopup">
                <dict:FilterWithPopupControl Filter="{Binding Path=Filter, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dgx:DataGridFilterColumnControl}}" />
            </ControlTemplate>
            <!--#endregion-->

            <!--#region Daty -->
            <ControlTemplate x:Key="FilterDatesWithPopup">
                <dict:FilterDataWithPopupControl Filter="{Binding Path=Filter, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dgx:DataGridFilterColumnControl}}" />
            </ControlTemplate>

            <!--#endregion-->
            <!--#endregion-->

            <!--#region LOKATOR -->
            <!-- Lokator do bindowania View z ViewModelami - kazdy View z tego korzysta -->
            <vm:ViewModelLocator x:Key="Locator" />
            <!--#endregion-->

            <ResourceDictionary.MergedDictionaries>

                <!-- Tutaj wpisujemy odwołania do globanych styli dla kontrolek w katalogu ResourceDictinaries -->

                <!--#region MaterialDesign -->
                <!--#region MaterialDesignThemes -->
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Dark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Primary/MaterialDesignColor.Indigo.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Accent/MaterialDesignColor.Indigo.xaml" />
                <!--#endregion-->

                <!--#region MaterialDesign Custom colors -->
                <ResourceDictionary>
                    <!-- primary -->
                    <!--<SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#2A5CAC"/>-->
                    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#929EB0"/>
                    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#FFFFFF"/>
                    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#2A5CAC"/>
                    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFFFFF"/>
                    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#2A5CAC"/>
                    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFF"/>
                    <!-- accent -->
                    <SolidColorBrush x:Key="SecondaryAccentBrush" Color="#D3D3D3"/>
                    <SolidColorBrush x:Key="SecondaryAccentForegroundBrush" Color="#2A5CAC"/>
                </ResourceDictionary>
                <!--#endregion-->

                <!--#region MaterialDesing Overrides -->
                <ResourceDictionary Source="ResourceDictionaries/MaterialDesign/MaterialDesignThemes.Overrides.xaml" />
                <!--#endregion-->
                <!--#endregion-->
                
                <!--#region Styles -->
                <ResourceDictionary Source="ResourceDictionaries/StyleDictionary.xaml" />
                <!--#endregion-->

                <!--#endregion-->

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
